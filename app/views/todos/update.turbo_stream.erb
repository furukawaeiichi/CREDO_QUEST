<%= turbo_stream.replace "todo_#{@todo.id}" do %>
  <%= form_with model: [@list, @todo] do |f| %>
    <%= f.check_box :checked, id: "todo_#{@todo.id}", class: "form-check-input me-1" %>
  <% end %>
<% end %>
<%= turbo_stream_flash %>
<%= turbo_stream.update "user_level" do %>
  Level: <%= @level %>
<% end %>
